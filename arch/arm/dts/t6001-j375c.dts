/dts-v1/;

#include "t6001.dtsi"
#include "t600x-j375.dtsi"

/ {
	compatible = "apple,j375c", "apple,t6001", "apple,arm-platform";
	model = "Apple Mac Studio (M1 Max, 2022)";
};

&pinctrl_ap {
	usb_hub_oe-hog {
		gpio-hog;
		gpios = <230 0>;
		input;
		line-name = "usb-hub-oe";
	};

	usb_hub_rst-hog {
		gpio-hog;
		gpios = <231 GPIO_ACTIVE_LOW>;
		output-low;
		line-name = "usb-hub-rst";
	};
};
